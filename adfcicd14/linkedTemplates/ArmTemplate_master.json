{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "adfcicd14"
		},
		"AzureblobLink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureblobLink'"
		},
		"Dev_Input_Blob_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "emp.txt"
		},
		"Dev_Input_Blob_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "cicd/input"
		},
		"Dev_Output_Blob_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"Dev_Output_Blob_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "cicd/output"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureblobLink_connectionString": {
						"value": "[parameters('AzureblobLink_connectionString')]"
					},
					"Dev_Input_Blob_properties_typeProperties_fileName": {
						"value": "[parameters('Dev_Input_Blob_properties_typeProperties_fileName')]"
					},
					"Dev_Input_Blob_properties_typeProperties_folderPath": {
						"value": "[parameters('Dev_Input_Blob_properties_typeProperties_folderPath')]"
					},
					"Dev_Output_Blob_properties_typeProperties_fileName": {
						"value": "[parameters('Dev_Output_Blob_properties_typeProperties_fileName')]"
					},
					"Dev_Output_Blob_properties_typeProperties_folderPath": {
						"value": "[parameters('Dev_Output_Blob_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}